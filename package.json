{
	"name": "riscv-extension",
	"displayName": "RISC-V Extension",
	"description": "RISC-V syntax highlighting and snippets",
	"version": "0.0.2",
	"publisher": "rihong",
	"engines": {
		"vscode": "^1.5.0"
	},
	"homepage": "https://github.com/Artessay/RISCV-Extension",
	"icon": "images/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/Artessay/RISCV-Extension"
	},
	"bugs": {
		"url": "https://github.com/Artessay/RISCV-Extension/issues",
		"email": "rihong.edu@outlook.com"
	},
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"contributes": {
		"languages": [
			{
				"id": "riscv",
				"aliases": [
					"RISC-V Assembly",
					"riscv"
				],
				"extensions": [
					".S",
					".s",
					".riscv",
					".asm"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "riscv",
				"scopeName": "source.riscv",
				"path": "./syntaxes/riscv.tmLanguage"
			}
		],
		"snippets": [
			{
				"language": "riscv",
				"path": "./snippets/snippets.json"
			}
		]
	}
}